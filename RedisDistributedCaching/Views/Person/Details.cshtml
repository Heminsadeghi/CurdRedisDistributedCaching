@model RedisDistributedCaching.Dto.PersonDto

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container p-3 my-3 border">
    <h1> Edit</h1>

    <div class="row">
        <div class="col-sm-6">
            <hr />
         
               
                <div class="form-group">
                    <label asp-for="FirstName" class="control-label"></label>
                <input asp-for="FirstName" class="form-control" readonly />
             
                </div>
                <div class="form-group">
                    <label asp-for="LastName" class="control-label"></label>
                <input asp-for="LastName" class="form-control" readonly />
                   
                </div>
                <div class="form-group">
                    <label asp-for="AdditionalContactInfo" class="control-label"></label>
                <input asp-for="AdditionalContactInfo" class="form-control" readonly />
             
                </div>
                <div class="form-group">
                    <label asp-for="Suffix" class="control-label"></label>
                <input asp-for="Suffix" class="form-control" readonly />
               
                </div>
                <div class="form-group">
                    <label asp-for="Email" class="control-label"></label>
                <input asp-for="Email" class="form-control" readonly />
                   
                </div>
                <div class="form-group">
                   
                    <a class="btn btn-danger" asp-action="Index">Back</a>
                </div>
       
        </div>
    </div>
</div>
@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}